<div class="span12">
	<table border="1"  class="table table-hover listings">
		<thead>
			<tr>
				<th>Item</th>
				<th>Image</th>
				<th>Description</th>
				<th>Price</th>
				<th>Bid #</th>
				<th>Delete #</th>
			</tr>
		</thead>
		<tbody>
			<% @listings.each do |listing| %>
				<tr class="admin-lists" onclick="document.location = '/listings/<%= listing.id %>/edit';" >
					<%= render 'admin_list_item', item: listing %>
				</tr>
			<% end %> 
		<tbody>
	</table>
</div>
