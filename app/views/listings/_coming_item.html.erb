<div class="well well-front coming-item">
  <div class="item-image">
    <a href="/listings/<%=item.id%>">
      <%= image_tag(item.photo.url(:square), :class => "img-responsive", :alt => "Responsive image") %> 
    </a>
  </div>
  <h2 class="coming-title"><%= item.title %></h2>
  <ul class="unstyled">
    <li class="coming-bid" id="listing-price-<%= item.id %>">Winning Bid: <strong class="pull-right">--</strong></li>
    <li class="coming-winning" id="listing-user-<%= item.id %>">Winner: <strong class="pull-right">--</strong></li>
  </ul>
</div>
<%= render 'bid_js', listing: item  %>